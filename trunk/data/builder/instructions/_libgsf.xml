<pack name="libgsf" license="">
  <meta>
    <homepage></homepage>
    <download>http://ftp.gnome.org/pub/gnome/sources/libgsf/</download>
    <!-- http://www.linuxfromscratch.org/blfs/view/svn/general/libgsf.html -->
    <descriptions>
      <description lang="en" type="short"></description>
      <description lang="en" type="long">The libgsf package contains the library used for providing an extensible input/output abstraction layer for structured file formats.</description>
      <description lang="de" type="short"></description>
      <description lang="de" type="long"></description>
    </descriptions>
    <keywords></keywords>
  </meta>

  <comments>
  </comments>

  <set version="1.14.27" date="2013-06-23" state="works">
<!--
Required: GLib-2.36.3, Intltool-0.50.2 and libxml2-2.9.1
Recommended: gdk-pixbuf-2.28.2 (To build gsf-office-thumbnailer)
Optional: gobject-introspection-1.36.0 and GTK-Doc-1.19 
-->

    <downloads>
      <download file="libgsf-1.14.27.tar.xz">
        <url checked="2013-07-30">http://ftp.gnome.org/pub/gnome/sources/libgsf/1.14/libgsf-1.14.27.tar.xz</url>
        <hash type="CRC32"></hash>
        <hash type="MD5"></hash>
        <hash type="SHA1"></hash>
        <hash type="SHA512"></hash>
        <hash type="RIPEMD256"></hash>
        <size></size>
      </download>
    </downloads>

    <commands>
PACKAGE_NAME=libgsf
PACKAGE_VERSION=1.14
oegenv 32
oegcleanbase
oegextract ${PACKAGE_NAME}-${PACKAGE_VERSION}.tar.xz
#oegimport libiconv-1.14
#oegimport gettext-0.18.3
configure --prefix=${OEG_BASE_DIR}
make
make install-strip DESTDIR=${OEG_INSTALL_DIR}
cd "${OEG_INSTALL_DIR}${OEG_BASE_DIR}"
oegarchive ${PACKAGE_NAME}-${PACKAGE_VERSION}
    </commands>
  </set>
</pack>

